<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kuretru.web.aries.mapper.WebSiteMapper">

    <select id="getMaxSequence" resultType="java.lang.Short">
        select max(sequence)
        from web_site
        where category_id = #{categoryId}
    </select>

</mapper>
